{
  "short_name": "Overwatch Stats",
  "name": "Overwatch Stats",
  "description": "Have a cry about your stats every time you open a new tab!",
  "manifest_version": 2,
  "icons": {
    "16": "ow-icon-16.png",
    "48": "ow-icon-48.png",
    "128": "ow-icon-128.png"
  },
  "browser_action": {
    "default_title": "Overwatch Stats"
  },
  "version": "1.6",
  "permissions": [
    "management",
    "tabs",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
